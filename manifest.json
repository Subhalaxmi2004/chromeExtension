{
    "manifest_version": 3,
    "name": "Title Picker Extension",
    "version": "1.0",
    "description": "A Chrome extension to pick up the title of the current tab.",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128":"icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  